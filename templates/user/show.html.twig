{% extends 'base.html.twig' %}

{% block title %}stage | Rechercher des amis{% endblock %}

{% block container %}

    <div id="user-login">

        {% for user in users %}
            <div>
                <p>{{ user.id}}</p>
                <p>
                <a href="{{ path('user_follow', {'id': user.id}) }}">
                {{ user.username}}
                </a>
                </p> 
            </div>
        {% endfor %}


    </div>

{% endblock %}